
public class ConstructGraph {
	
	int[][] graph;
	int[] vertices;
	
	public ConstructGraph(int[][] graphConections, int[] inputVertices) {
		graph = graphConections;
		vertices = inputVertices;
	}
	
	public boolean graphCheck() {
		if (graph.length == vertices.length) {
			return false;
		}else {
			return true;
		}
	}
	
	
}
