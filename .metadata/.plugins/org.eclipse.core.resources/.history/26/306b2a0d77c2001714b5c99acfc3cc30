
import java.util.*;
public class Cash {

	public static void main(String[] args) {
		int cash = 0;
		Scanner input = new Scanner(System.in);
		
		cash = input.nextInt();
		System.out.println(whatCoin(cash));
		
	}
	public static int[] whatCoin(int cash) {
		int[] denom = {25,10,5,1};
		int[] result = {0,0,0,0};
		int i = 0;
		
		while(cash != 0) {
			result[i] =  (int) (cash / denom[i]);
			cash -= (int) (cash / denom[i]);
			i++;
		}
		return result;
	}
}
